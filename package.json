{
  "name": "swg-trn-viewer",
  "displayName": "SWG Terrain Viewer",
  "description": "View and query SWG terrain (.trn) files - find boundaries at coordinates",
  "version": "1.0.0",
  "publisher": "swgemu",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "swgemu.trnViewer",
        "displayName": "SWG Terrain Viewer",
        "selector": [
          {
            "filenamePattern": "*.trn"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^5.0.0"
  }
}
